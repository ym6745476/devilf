  "code_generation": {
    "patterns": [
      {
        "name": "نمط عام",
        "format": "ALFATH-{RANDOM:6}-{RANDOM:4}",
        "example": "ALFATH-A7B3C9-X4Y2"
      },
      {
        "name": "نمط حدث",
        "format": "ALFATH-{EVENT}-{YEAR}-{RANDOM:4}",
        "example": "ALFATH-RAMADAN-2025-X4Y2"
      },
      {
        "name": "نمط شريك",
        "format": "{PARTNER}-ALFATH-{RANDOM:6}",
        "example": "YOUTUBER-ALFATH-A7B3C9"
      },
      {
        "name": "نمط شخصي",
        "format": "{PLAYERID:4}-ALFATH-{RANDOM:6}",
        "example": "P123-ALFATH-A7B3C9"
